{
  "name": "CuteSquare",
  "version": "1.0.0",
  "description": "Try to Build my 1st Extension!",
  "permissions": ["activeTab","declarativeContent","storage"],
  //declare the "storage" permission in the extension manifest to use the storage API
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
      "default_icon": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
        //small icon on toolbars have setting up
      }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  //Large icon have be setting up on the developer pages
  "options_page": "option.html",
  "manifest_version": 2
  //Manifest version must be 2
}
